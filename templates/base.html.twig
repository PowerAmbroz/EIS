<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('style/css/bootstrap.css') }}">
            <link rel="stylesheet" href="{{ asset('style/css/bootstrap-grid.css') }}">
            <link rel="stylesheet" href="{{ asset('style/css/bootstrap-reboot.css') }}">
            <link rel="stylesheet" href="{{ asset('style/css/custom.css') }}">

            <script src="{{ asset('style/js/jquery-3.5.1.min.js') }}"></script>
            <script src="{{ asset('style/js/jquery.tablesorter.min.js') }}"></script>

        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}

            <script src="{{ asset('style/js/bootstrap.bundle.js') }}"></script>
            <script src="{{ asset('style/js/bootstrap.js') }}"></script>

            {% endblock %}
    </body>
</html>
